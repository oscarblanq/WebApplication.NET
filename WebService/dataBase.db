BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS `subjects` (
	`name`	TEXT,
	`time`	INTEGER,
	`credits`	INTEGER,
	`idSubject`	INTEGER
);
CREATE TABLE IF NOT EXISTS `students` (
	`IDstudent`	INTEGER,
	`Name`	TEXT,
	`Age`	INTEGER,
	`password`	TEXT
);
CREATE TABLE IF NOT EXISTS `professor` (
	`IDprofessor`	INTEGER,
	`name`	TEXT,
	`password`	TEXT
);
COMMIT;
